			<if test="productId != null">`product_id` = #{productId}, </if>
			<if test="materialName != null">`material_name` = #{materialName}, </if>
			<if test="specifical != null">`specifical` = #{specifical}, </if>
			<if test="model != null">`model` = #{model}, </if>
